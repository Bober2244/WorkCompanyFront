{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport ModalWindow from \"@/components/ModalWindow.vue\";\nimport CartShop from \"@/components/CartShop.vue\";\nimport { mapGetters } from \"vuex\";\nimport store from \"../store\";\nexport default {\n  components: {\n    CartShop,\n    ModalWindow\n  },\n  data() {\n    return {\n      isCartOpen: false // Состояние для открытия/закрытия корзины\n    };\n  },\n\n  computed: {\n    ...mapGetters([\"totalPrice\"])\n  },\n  methods: {\n    toggleCart() {\n      this.isCartOpen = !this.isCartOpen;\n    },\n    closeCart() {\n      this.isCartOpen = false;\n    },\n    showLogoutModal() {\n      this.$refs.confirmationModal.openModal();\n    },\n    logout() {\n      store.dispatch('logout');\n      sessionStorage.removeItem('userId');\n      sessionStorage.removeItem(\"authToken\");\n      this.$router.push(\"/\");\n    },\n    formatPrice(price) {\n      return price > 0 ? `${price} руб.` : \"0 руб.\";\n    }\n  }\n};","map":{"version":3,"mappings":";AAuCA,OAAOA,WAAU,MAAO,8BAA8B;AACtD,OAAOC,QAAO,MAAO,2BAA2B;AAChD,SAAQC,UAAU,QAAO,MAAM;AAC/B,OAAOC,KAAI,MAAO,UAAU;AAE5B,eAAe;EACbC,UAAU,EAAE;IACVH,QAAQ;IACRD;EACF,CAAC;EACDK,IAAIA,GAAG;IACL,OAAO;MACLC,UAAU,EAAE,KAAK,CAAE;IACrB,CAAC;EACH,CAAC;;EACDC,QAAQ,EAAE;IACR,GAAGL,UAAU,CAAC,CAAC,YAAY,CAAC;EAC9B,CAAC;EACDM,OAAO,EAAE;IACPC,UAAUA,GAAG;MACX,IAAI,CAACH,UAAS,GAAI,CAAC,IAAI,CAACA,UAAU;IACpC,CAAC;IACDI,SAASA,GAAG;MACV,IAAI,CAACJ,UAAS,GAAI,KAAK;IACzB,CAAC;IACDK,eAAeA,GAAG;MAChB,IAAI,CAACC,KAAK,CAACC,iBAAiB,CAACC,SAAS,CAAC,CAAC;IAC1C,CAAC;IACDC,MAAMA,GAAG;MACPZ,KAAK,CAACa,QAAQ,CAAC,QAAQ,CAAC;MAExBC,cAAc,CAACC,UAAU,CAAC,QAAQ,CAAC;MACnCD,cAAc,CAACC,UAAU,CAAC,WAAW,CAAC;MACtC,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,GAAG,CAAC;IACxB,CAAC;IACDC,WAAWA,CAACC,KAAK,EAAE;MACjB,OAAOA,KAAI,GAAI,IAAK,GAAEA,KAAK,OAAM,GAAI,QAAQ;IAC/C;EACF;AACF,CAAC","names":["ModalWindow","CartShop","mapGetters","store","components","data","isCartOpen","computed","methods","toggleCart","closeCart","showLogoutModal","$refs","confirmationModal","openModal","logout","dispatch","sessionStorage","removeItem","$router","push","formatPrice","price"],"sourceRoot":"","sources":["/Users/nicolassergeev/WebstormProjects/frontend-ShoesShop/src/components/AppHeader.vue"],"sourcesContent":["<template>\n  <header class=\"header\">\n    <div class=\"flex items-center gap-4\">\n      <router-link to=\"/home\">\n        <img src=\"/logo.png\" alt=\"Logo\" class=\"w-11 cursor-pointer\" />\n      </router-link>\n      <div>\n        <h2 class=\"font-bold text-xl uppercase\">Сити-Класс</h2>\n        <p class=\"text-slate-400\">Лучший магазин обуви</p>\n      </div>\n    </div>\n\n    <ul class=\"flex items-center gap-10\">\n      <li class=\"flex items-center gap-3 cursor-pointer\" @click=\"toggleCart\">\n        <img src=\"/cart.svg\" alt=\"Cart\" />\n        <b>{{ formatPrice(store.totalPrice) }}</b>\n      </li>\n      <router-link to=\"/favorites\" class=\"no-underline\">\n        <li class=\"flex items-center gap-3 cursor-pointer\">\n          <img src=\"/heart.svg\" alt=\"Favorite\" />\n          <span>Закладки</span>\n        </li>\n      </router-link>\n      <li class=\"flex items-center gap-3 cursor-pointer\" @click=\"showLogoutModal\">\n        <img src=\"/profile.svg\" alt=\"Favorite\" />\n        <span>Выйти из аккаунта</span>\n      </li>\n    </ul>\n\n    <ModalWindow\n        ref=\"confirmationModal\"\n        @confirm=\"logout\"\n    />\n\n    <CartShop :is-open=\"isCartOpen\" @close=\"closeCart\" />\n  </header>\n</template>\n\n<script>\nimport ModalWindow from \"@/components/ModalWindow.vue\";\nimport CartShop from \"@/components/CartShop.vue\";\nimport {mapGetters} from \"vuex\";\nimport store from \"../store\";\n\nexport default {\n  components: {\n    CartShop,\n    ModalWindow\n  },\n  data() {\n    return {\n      isCartOpen: false, // Состояние для открытия/закрытия корзины\n    };\n  },\n  computed: {\n    ...mapGetters([\"totalPrice\"]),\n  },\n  methods: {\n    toggleCart() {\n      this.isCartOpen = !this.isCartOpen;\n    },\n    closeCart() {\n      this.isCartOpen = false;\n    },\n    showLogoutModal() {\n      this.$refs.confirmationModal.openModal();\n    },\n    logout() {\n      store.dispatch('logout');\n\n      sessionStorage.removeItem('userId');\n      sessionStorage.removeItem(\"authToken\");\n      this.$router.push(\"/\");\n    },\n    formatPrice(price) {\n      return price > 0 ? `${price} руб.` : \"0 руб.\";\n    },\n  }\n};\n</script>\n\n<style scoped>\n.no-underline {\n  text-decoration: none !important;\n  outline: none !important;\n  box-shadow: none !important;\n}\n\n.no-underline:hover,\n.no-underline:focus {\n  text-decoration: none !important;\n  outline: none !important;\n  box-shadow: none !important;\n}\n\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}