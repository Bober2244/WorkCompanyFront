{"ast":null,"code":"import { ref, computed } from 'vue';\nimport BidItem from \"@/components/BidItem.vue\";\n\n// Данные для заявок\n\nexport default {\n  __name: 'CustomerScreen',\n  setup(__props, {\n    expose\n  }) {\n    expose();\n\n    // Добавляем необходимые импорты\n    const customers = ref([{\n      name: 'John Doe',\n      email: 'john@example.com',\n      phone: '+1234567890',\n      image: '1.jpg'\n    }, {\n      name: 'Jane Smith',\n      email: 'jane@example.com',\n      phone: '+0987654321',\n      image: '2.jpg'\n    }, {\n      name: 'Bob Johnson',\n      email: 'bob@example.com',\n      phone: '+1122334455',\n      image: '3.jpg'\n    }]);\n\n    // Переменная для хранения поискового запроса\n    const searchQuery = ref('');\n    const filteredCustomers = computed(() => {\n      return customers.value.filter(customer => {\n        return customer.name.toLowerCase().includes(searchQuery.value.toLowerCase()) || customer.email.toLowerCase().includes(searchQuery.value.toLowerCase()) || customer.phone.includes(searchQuery.value);\n      });\n    });\n    const __returned__ = {\n      customers,\n      searchQuery,\n      filteredCustomers,\n      ref,\n      computed,\n      BidItem\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"mappings":"AAEA,SAASA,GAAG,EAAEC,QAAQ,QAAQ,KAAK;AACnC,OAAOC,OAAO,MAAM,0BAA0B;;AAE9C;;;;;;;;;IAJA;IAKA,MAAMC,SAAS,GAAGH,GAAG,CAAC,CACpB;MAAEI,IAAI,EAAE,UAAU;MAAEC,KAAK,EAAE,kBAAkB;MAAEC,KAAK,EAAE,aAAa;MAAEC,KAAK,EAAE;IAAQ,CAAC,EACrF;MAAEH,IAAI,EAAE,YAAY;MAAEC,KAAK,EAAE,kBAAkB;MAAEC,KAAK,EAAE,aAAa;MAAEC,KAAK,EAAE;IAAQ,CAAC,EACvF;MAAEH,IAAI,EAAE,aAAa;MAAEC,KAAK,EAAE,iBAAiB;MAAEC,KAAK,EAAE,aAAa;MAAEC,KAAK,EAAE;IAAQ,CAAC,CACxF,CAAC;;IAEF;IACA,MAAMC,WAAW,GAAGR,GAAG,CAAC,EAAE,CAAC;IAG3B,MAAMS,iBAAiB,GAAGR,QAAQ,CAAC,MAAM;MACvC,OAAOE,SAAS,CAACO,KAAK,CAACC,MAAM,CAACC,QAAQ,IAAI;QACxC,OAAOA,QAAQ,CAACR,IAAI,CAACS,WAAW,CAAC,CAAC,CAACC,QAAQ,CAACN,WAAW,CAACE,KAAK,CAACG,WAAW,CAAC,CAAC,CAAC,IACxED,QAAQ,CAACP,KAAK,CAACQ,WAAW,CAAC,CAAC,CAACC,QAAQ,CAACN,WAAW,CAACE,KAAK,CAACG,WAAW,CAAC,CAAC,CAAC,IACtED,QAAQ,CAACN,KAAK,CAACQ,QAAQ,CAACN,WAAW,CAACE,KAAK,CAAC;MAChD,CAAC,CAAC;IACJ,CAAC,CAAC","names":["ref","computed","BidItem","customers","name","email","phone","image","searchQuery","filteredCustomers","value","filter","customer","toLowerCase","includes"],"sources":["/Users/nicolassergeev/WebstormProjects/MegaProject/pages/Screens/CustomerScreen.vue"],"sourcesContent":["<script setup>\n// Добавляем необходимые импорты\nimport { ref, computed } from 'vue';\nimport BidItem from \"@/components/BidItem.vue\";\n\n// Данные для заявок\nconst customers = ref([\n  { name: 'John Doe', email: 'john@example.com', phone: '+1234567890', image: '1.jpg' },\n  { name: 'Jane Smith', email: 'jane@example.com', phone: '+0987654321', image: '2.jpg' },\n  { name: 'Bob Johnson', email: 'bob@example.com', phone: '+1122334455', image: '3.jpg' },\n]);\n\n// Переменная для хранения поискового запроса\nconst searchQuery = ref('');\n\n\nconst filteredCustomers = computed(() => {\n  return customers.value.filter(customer => {\n    return customer.name.toLowerCase().includes(searchQuery.value.toLowerCase()) ||\n        customer.email.toLowerCase().includes(searchQuery.value.toLowerCase()) ||\n        customer.phone.includes(searchQuery.value);\n  });\n});\n</script>\n\n<template>\n  <h1 class=\"my-1\">Заявки</h1>\n  <div class=\"container\">\n\n    <div class=\"search-bar my-3\">\n      <input type=\"text\" v-model=\"searchQuery\" class=\"form-control\" placeholder=\"Поиск по имени, email или телефону\" />\n    </div>\n    <div class=\"customers__list table my-3\">\n      <div class=\"customers__titlebar dflex justify-content-between align-items-center\">\n        <div class=\"customers__titlebar--item\">\n\n        </div>\n        <div class=\"customers__titlebar--item\">\n          <button class=\"btn btn-secondary my-1\">\n            Создать заявку\n          </button>\n        </div>\n      </div>\n\n      <div class=\"table--heading mt-2 customers__list__heading\" style=\"padding-top: 20px;background:#FFF\">\n        <p class=\"table--heading--col1\">Image</p>\n        <p class=\"table--heading--col2\">Customer Name</p>\n        <p class=\"table--heading--col3\">Email</p>\n        <p class=\"table--heading--col4\">Phone</p>\n        <p class=\"table--heading--col5\">Actions</p>\n      </div>\n\n      <!-- Цикл по заявкам -->\n      <div v-for=\"customer in filteredCustomers\" :key=\"customer.email\">\n        <BidItem :customer=\"customer\" />\n      </div>\n    </div>\n  </div>\n</template>\n\n<style scoped>\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n  font-family: 'Fira sans', sans-serif;\n}\n\nbody {\n  background: #6c5ce7;\n  color: #FFF;\n}\n\n.container {\n  width: 100%;\n  max-width: 780px;\n  margin-top: 5rem;\n  margin-left: auto;\n  margin-right: auto;\n}\n\n/* Styling for customer and order sections */\n.customers__list, .orders__list {\n  margin-top: 20px;\n}\n\n.search-bar input {\n  padding: 10px;\n  font-size: 16px;\n  width: 100%;\n  border-radius: 4px;\n  border: 1px solid #ddd;\n}\n\n.table--heading {\n  display: grid;\n  grid-template-columns: repeat(6, 1fr);\n  color: #6a727a;\n  font-size: 14px;\n  font-weight: 500;\n  border-bottom: 1px solid #e0e0e0;\n  background-color: #f1f1f1;\n}\n\n.btn-icon {\n  width: 35px;\n  height: 35px;\n  border-radius: 4px;\n  color: #6a727a;\n  cursor: pointer;\n}\n\n.btn-icon-success:hover {\n  background: rgb(47, 214, 111);\n  color: white;\n}\n\n.btn-icon-danger:hover {\n  background: crimson;\n  color: white;\n}\n\n.btn-secondary {\n  background: #FFF;\n  color: black;\n}\n\n.btn-secondary:hover {\n  background: #f1f1f1;\n  color: #6a727a;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}