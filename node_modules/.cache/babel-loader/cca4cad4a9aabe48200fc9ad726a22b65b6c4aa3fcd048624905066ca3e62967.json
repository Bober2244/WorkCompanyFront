{"ast":null,"code":"import AppHeader from \"@/components/AppHeader.vue\";\nimport AppFooter from \"@/components/AppFooter.vue\";\nimport axios from \"axios\";\nexport default {\n  name: 'ContactPage',\n  components: {\n    AppFooter,\n    AppHeader\n  },\n  data() {\n    return {\n      form: {\n        name: '',\n        email: '',\n        message: ''\n      },\n      feedbackSent: false,\n      errorMessage: ''\n    };\n  },\n  methods: {\n    async submitForm() {\n      try {\n        const response = await axios.post('http://localhost:8000/api/feedback', this.form);\n        if (response.data.status === 'success') {\n          this.feedbackSent = true;\n          this.errorMessage = '';\n        }\n      } catch (error) {\n        this.errorMessage = 'Ошибка при отправке формы. Попробуйте позже.';\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AACA,OAAOA,SAAQ,MAAO,4BAA4B;AAClD,OAAOC,SAAQ,MAAO,4BAA4B;AAClD,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,UAAU,EAAE;IAACH,SAAS;IAAED;EAAS,CAAC;EAClCK,IAAIA,GAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJH,IAAI,EAAE,EAAE;QACRI,KAAK,EAAE,EAAE;QACTC,OAAO,EAAE;MACX,CAAC;MACDC,YAAY,EAAE,KAAK;MACnBC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,UAAUA,GAAG;MACjB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMX,KAAK,CAACY,IAAI,CAAC,oCAAoC,EAAE,IAAI,CAACR,IAAI,CAAC;QAClF,IAAIO,QAAQ,CAACR,IAAI,CAACU,MAAK,KAAM,SAAS,EAAE;UACtC,IAAI,CAACN,YAAW,GAAI,IAAI;UACxB,IAAI,CAACC,YAAW,GAAI,EAAE;QACxB;MACF,EAAE,OAAOM,KAAK,EAAE;QACd,IAAI,CAACN,YAAW,GAAI,8CAA8C;MACpE;IACF;EACF;AACF,CAAC","names":["AppHeader","AppFooter","axios","name","components","data","form","email","message","feedbackSent","errorMessage","methods","submitForm","response","post","status","error"],"sourceRoot":"","sources":["/Users/nicolassergeev/WebstormProjects/frontend-ShoesShop/pages/Contacts.vue"],"sourcesContent":["<script>\nimport AppHeader from \"@/components/AppHeader.vue\";\nimport AppFooter from \"@/components/AppFooter.vue\";\nimport axios from \"axios\";\n\nexport default {\n  name: 'ContactPage',\n  components: {AppFooter, AppHeader},\n  data() {\n    return {\n      form: {\n        name: '',\n        email: '',\n        message: '',\n      },\n      feedbackSent: false,\n      errorMessage: '',\n    };\n  },\n  methods: {\n    async submitForm() {\n      try {\n        const response = await axios.post('http://localhost:8000/api/feedback', this.form);\n        if (response.data.status === 'success') {\n          this.feedbackSent = true;\n          this.errorMessage = '';\n        }\n      } catch (error) {\n        this.errorMessage = 'Ошибка при отправке формы. Попробуйте позже.';\n      }\n    },\n  },\n};\n</script>\n\n<template>\n  <AppHeader></AppHeader>\n  <main class=\"contact-page\">\n    <div class=\"content-container\">\n      <h1 class=\"contact-title\">Контакты</h1>\n      <p class=\"contact-intro\">Мы всегда рады вашим вопросам, предложениям и отзывам! Пожалуйста, свяжитесь с нами любым удобным способом, и мы постараемся ответить вам в кратчайшие сроки.</p>\n\n      <!-- Contact Form Section -->\n      <section class=\"contact-form-section\">\n        <h2 class=\"section-title\">Форма обратной связи</h2>\n        <p>Вы также можете отправить нам сообщение, заполнив форму ниже:</p>\n        <form @submit.prevent=\"submitForm\" class=\"contact-form\">\n          <div class=\"form-group\">\n            <label for=\"name\">Ваше имя:</label>\n            <input type=\"text\" id=\"name\" v-model=\"form.name\" required />\n          </div>\n          <div class=\"form-group\">\n            <label for=\"email\">Ваша почта:</label>\n            <input type=\"email\" id=\"email\" v-model=\"form.email\" required />\n          </div>\n          <div class=\"form-group\">\n            <label for=\"message\">Ваше сообщение:</label>\n            <textarea id=\"message\" v-model=\"form.message\" rows=\"4\" required></textarea>\n          </div>\n          <button class=\"form__button\">Отправить</button>\n        </form>\n\n        <!-- Success/Error Message -->\n        <div v-if=\"feedbackSent\" class=\"success-message\">Спасибо! Ваше сообщение отправлено.</div>\n        <div v-if=\"errorMessage\" class=\"error-message\">{{ errorMessage }}</div>\n      </section>\n    </div>\n  </main>\n  <AppFooter></AppFooter>\n</template>\n\n<style scoped lang=\"scss\">\n.success-message {\n  color: green;\n  font-weight: bold;\n  margin-top: 10px;\n}\n\n.error-message {\n  color: red;\n  font-weight: bold;\n  margin-top: 10px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}